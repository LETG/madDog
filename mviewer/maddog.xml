<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application 
        title="Mad Dog" 
        mouseposition="false" 
        logo="apps/maddog/img/maddog_logo.svg" 
        help="apps/maddog/accueil.html" 
        showhelp="false" 
        titlehelp=" " 
        iconhelp="home"
        measuretools="true" 
        exportpng="true" 
        style="apps/maddog/css/maddog-theme.css" />
    <mapoptions maxzoom="19" projection="EPSG:3857" center="-475802.67385310325, 6177784.8613143535" zoom="9" projextent="-20037508.342789244, -20037508.342789244, 20037508.342789244, 20037508.342789244" />
  
    <baselayers style="default"><!-- style="default"||gallery" -->
        <baselayer  type="OSM" id="darkmatter" label="Dark Matter" title="CartoDb" thumbgallery="img/basemap/darkmatter.png" 
            url="http://{a-c}.basemaps.cartocdn.com/dark_all/{z}/{x}/{y}.png" maxzoom="20" 
            visible="false" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="positron" label="Positron" title="CartoDb" thumbgallery="img/basemap/positron.png" 
            url="http://{a-c}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png" maxzoom="20" 
            visible="true" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="osm1" label="OpenStreetMap" title="OpenStreetMap" thumbgallery="img/basemap/osm.png" 
            url="http://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png" 
            attribution="© MapQuest. Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'>OpenStreetMap &lt;/a>,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a>" visible="false"/>
    </baselayers>     
    <proxy url=""/>
    <olscompletion url="https://api-adresse.data.gouv.fr/search/" type="ban"/>
    <searchparameters bbox="false" localities="false" features="false" />   

    <extensions>
        <extension type="component" id="wfs2Fuse" path="apps/maddog/addons"/>
        <extension type="component" id="axios" path="apps/maddog/addons"/>
        <extension type="component" id="maddog" path="apps/maddog/addons"/>
        <extension type="component" id="wps-js-52-north" path="apps/maddog/addons"/>
        <extension type="javascript" src="apps/maddog/addons/wps-js/wps-js-all.min.js"/>
    </extensions>

    <themes legendmini="true">         
        <theme name="Emprises"  collapsed="false" id="emprise" icon="fas fa-arrow-right">
        	<layer id="communes_intersect" name="Emprises des lignes de référence"  visible="true" 
                infoformat="application/vnd.ogc.gml"
                index="2"
                url="https://gis.jdev.fr/geoserver/maddog/wms"
                legendurl="" 
                queryable="true"
                infopanel="bottom-panel"
                attribution="Source: indigeo"
                metadata=""
                metadata-csw="" >
		    </layer>                           
        </theme>
        <theme name="Traits"  collapsed="false" id="tdc" icon="fas fa-arrow-right">
        	<layer id="trait_de_cote" name="Trait de côte"  visible="false"
                index="3"
                infoformat="application/vnd.ogc.gml"
                infopanel="bottom-panel"
                url="https://portail.indigeo.fr/geoserver/LETG-BREST/wms"
                legendurl=""
                queryable="true" 
                attribution="Source: indigeo"
                tooltip="true" tooltipenabled="true" tooltipcontent="{{nom}}"
                metadata=""
                metadata-csw="" >
		    </layer>                               
        	<layer id="emprises_buffer" name="Communes des lignes de référence"  visible="true" 
                index="1"
                tooltip="true" tooltipenabled="true" tooltipcontent="{{id}}"
                infopanel="bottom-panel"
                infoformat="application/vnd.ogc.gml"
                url="https://gis.jdev.fr/geoserver/maddog/wms"
                legendurl="" 
                queryable="true"
                attribution="Source: indigeo"
                metadata=""
                metadata-csw="" >
		    </layer>
            <layer id="communes" name="Communes littoral" visible="true" queryable="true"
                showintoc="true"
                type="customlayer" opacity="0.8"
                url="apps/maddog/customlayers/communes.js"
                vectorlegend="false"
                expanded="true"
                tooltip="true" tooltipenabled="true" tooltipcontent="{{nom}} "
                attribution="Source: iuem.univ-brest.fr"
                metadata=""
                metadata-csw="">
            </layer>
        </theme>
    </themes>
</config>